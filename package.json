{
  "name": "photo-gallery",
  "private": true,
  "version": "1.4.0",
  "type": "module",
  "scripts": {
    "start": "vite",
    "dev": "vite",
    "build": "tsc && vite build",
    "build-server": "cd server && CGO_ENABLED=1 go build -v -installsuffix cgo -o photo-gallery",
    "lint": "eslint src --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "preview": "vite preview",
    "server": "cd server && go build && ./photo-gallery",
    "server-dev": "nodemon --ext go --watch './server/' --signal SIGTERM --exec 'cd server && go build && ./photo-gallery'"
  },
  "dependencies": {
    "@emotion/react": "^11.11.1",
    "@emotion/styled": "^11.11.0",
    "@fontsource/pt-sans": "^5.0.3",
    "@mui/icons-material": "^5.11.16",
    "@mui/material": "^5.13.6",
    "@reduxjs/toolkit": "^1.9.5",
    "@rpldy/upload-drop-zone": "^1.4.1",
    "@rpldy/upload-paste": "^1.4.1",
    "@rpldy/upload-preview": "^1.4.1",
    "@rpldy/uploady": "^1.4.1",
    "leaflet": "^1.9.4",
    "notistack": "v3.0.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-leaflet": "^4.2.1",
    "react-photo-album": "^2.2.2",
    "react-redux": "^8.1.1",
    "react-router-dom": "^6.14.0",
    "react-virtualized-auto-sizer": "^1.0.20",
    "react-window": "^1.8.9",
    "yet-another-react-lightbox": "^3.11.3"
  },
  "devDependencies": {
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/react": "^14.0.0",
    "@types/jest": "^29.5.2",
    "@types/leaflet": "^1.9.3",
    "@types/node": "^20.3.2",
    "@types/react": "^18.2.14",
    "@types/react-dom": "^18.2.6",
    "@types/react-window": "^1.8.5",
    "@typescript-eslint/eslint-plugin": "^5.60.1",
    "@typescript-eslint/parser": "^5.60.1",
    "@vitejs/plugin-react": "^4.0.1",
    "eslint": "^8.43.0",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.4.1",
    "nodemon": "^2.0.22",
    "sass": "^1.63.6",
    "typescript": "5.0.2",
    "vite": "^4.3.9",
    "vite-plugin-eslint": "^1.8.1"
  }
}
