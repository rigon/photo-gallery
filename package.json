{
  "name": "photo-gallery",
  "private": true,
  "version": "1.5.0",
  "type": "module",
  "scripts": {
    "start": "vite",
    "dev": "vite",
    "build": "tsc && vite build",
    "build-server": "cd server && CGO_ENABLED=1 go build -v -installsuffix cgo -o photo-gallery",
    "lint": "eslint src --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "preview": "vite preview",
    "server": "cd server && go build && ./photo-gallery",
    "server-dev": "nodemon --ext go --watch './server/' --signal SIGTERM --exec 'cd server && go build && ./photo-gallery'"
  },
  "dependencies": {
    "@emotion/react": "^11.11.1",
    "@emotion/styled": "^11.11.0",
    "@fontsource/pt-sans": "^5.0.8",
    "@mui/icons-material": "^5.14.3",
    "@mui/material": "^5.14.3",
    "@reduxjs/toolkit": "^1.9.5",
    "leaflet": "^1.9.4",
    "notistack": "v3.0.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-leaflet": "^4.2.1",
    "react-photo-album": "^2.2.2",
    "react-redux": "^8.1.2",
    "react-router-dom": "^6.14.2",
    "react-virtualized-auto-sizer": "^1.0.20",
    "react-window": "^1.8.9",
    "yet-another-react-lightbox": "^3.12.0"
  },
  "devDependencies": {
    "@testing-library/jest-dom": "^5.17.0",
    "@testing-library/react": "^14.0.0",
    "@types/jest": "^29.5.3",
    "@types/leaflet": "^1.9.3",
    "@types/node": "^20.4.8",
    "@types/react": "^18.2.19",
    "@types/react-dom": "^18.2.7",
    "@types/react-window": "^1.8.5",
    "@typescript-eslint/eslint-plugin": "^6.3.0",
    "@typescript-eslint/parser": "^6.3.0",
    "@vitejs/plugin-react": "^4.0.4",
    "eslint": "^8.46.0",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.4.3",
    "nodemon": "^3.0.1",
    "sass": "^1.64.2",
    "typescript": "5.1.6",
    "vite": "^4.4.9",
    "vite-plugin-eslint": "^1.8.1"
  }
}
